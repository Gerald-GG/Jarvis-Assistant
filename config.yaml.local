assistant:
  name: "Jarvis"
  wake_word: "jarvis"
  
llm:
  provider: "openai"  # openai, anthropic, local
  model: "gpt-3.5-turbo"
  temperature: 0.7

speech:
  stt_engine: "whisper"  # whisper, google, local
  tts_engine: "pyttsx3"  # elevenlabs, google, pyttsx3
  language: "en-US"
  
memory:
  enabled: true
  max_history: 10